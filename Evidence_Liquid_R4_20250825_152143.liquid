{% mergeDiff msg -%}
{% if msg.resourceType == "Evidence" -%}
  {% assign fields = "" | split: "" %}
  {% if msg.shortTitle == null -%}
    {% assign fields = fields | push: '"shortTitle" : null' %}
  {% endif -%}
  {% if msg.subtitle == null -%}
    {% assign fields = fields | push: '"subtitle" : null' %}
  {% endif -%}
  {% if msg.jurisdiction == null -%}
    {% assign fields = fields | push: '"jurisdiction" : null' %}
  {% endif -%}
  {% if msg.effectivePeriod == null -%}
    {% assign fields = fields | push: '"effectivePeriod" : null' %}
  {% endif -%}
  {% if msg.topic == null -%}
    {% assign fields = fields | push: '"topic" : null' %}
  {% endif -%}
  {% if msg.exposureBackground == null -%}
    {% assign fields = fields | push: '"exposureBackground" : null' %}
  {% endif -%}
  {% if msg.exposureVariant == null -%}
    {% assign fields = fields | push: '"exposureVariant" : null' %}
  {% endif -%}
  {% if msg.outcome == null -%}
    {% assign fields = fields | push: '"outcome" : null' %}
  {% endif -%}
  {{ fields | join: "," }}
{% endif %}
{% endmergeDiff %}
